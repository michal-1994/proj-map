{
    "name": "proj-map",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "prettier": "npx prettier --write .",
        "predeploy": "npm run build --base-href=/proj-map/",
